<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smileShark.mapper.QuestionAndAnswerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.smileShark.entity.QuestionAndAnswer">
        <id column="question_id" property="questionId" />
        <result column="question" property="question" />
        <result column="answers" property="answers" />
        <result column="start_time" property="startTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        question_id, question, answers, start_time
    </sql>

</mapper>
